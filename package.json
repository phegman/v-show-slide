    "easing",
    "easings",
    "directive",
    "slide up",
    "slide down",
    "slideUp",
    "slideDown"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/phegman/v-show-slide.git"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.1.0",
